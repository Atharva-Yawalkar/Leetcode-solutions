class Solution {
public:
    void reverseString(vector<char>& s) {
        // // int j=s.size()-1;
        // for (int i=0,j=s.size()-1; i<s.size(),j>0;i++,j--){
        //     s[i]=s[j];
        //     // swap(s[i],s[j]);
        int i=0;
        int j=s.size()-1;
        while (i<j)
        swap(s[i++],s[j--]);
    }
};
